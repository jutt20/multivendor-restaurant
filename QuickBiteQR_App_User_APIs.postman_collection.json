		"_postman_id": "quickbiteqr-app-user-apis",
		"name": "QuickBiteQR - App User APIs",
		"description": "Complete API collection for QuickBiteQR mobile app users. Includes authentication, profile management, address management, restaurant browsing, cart operations, and order management (delivery, pickup, dine-in).",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
